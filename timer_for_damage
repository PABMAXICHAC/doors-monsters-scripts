local broom = game.ReplicatedStorage.GameData.LatestRoom.Value
local timer = 0
local finishtimer = 120
repeat
wait(1)
timer = timer + 1
until timer == finishtimer
--
--
while true do
if game.ReplicatedStorage.GameData.LatestRoom.Changed then 
timer = 0
break end
if timer >= 300 then
loadstring(game:HttpGet("https://github.com/PABMAXICHAC/doors-monsters-scripts/raw/main/deal_damage_until_door_open"))()
break end
end
